<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
        xmlns="http://www.springframework.org/schema/beans"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://dubbo.apache.org/schema/dubbo
        http://dubbo.apache.org/schema/dubbo/dubbo.xsd"
>

    <dubbo:application name="familyhelper-plugin-notify-test">
        <dubbo:parameter key="qos.enable" value="false"/>
    </dubbo:application>

    <!-- zookeeper注册中心 -->
    <dubbo:registry address="${dubbo.registry.zookeeper.address}"/>

    <!-- dubbo协议 -->
    <dubbo:protocol name="dubbo" port="${dubbo.protocol.dubbo.port}" host="${dubbo.protocol.dubbo.host}"
                    dispatcher="message" threadpool="fixed" threads="100"/>

    <!-- 向注册中心注册暴露服务地址,注册消费者服务 -->
    <!-- notify -->
    <dubbo:reference id="notifyNotifySettingMaintainService"
                     interface="com.dwarfeng.notify.stack.service.NotifySettingMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="notifyTopicMaintainService"
                     interface="com.dwarfeng.notify.stack.service.TopicMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="notifyRouterInfoMaintainService"
                     interface="com.dwarfeng.notify.stack.service.RouterInfoMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="notifySenderInfoMaintainService"
                     interface="com.dwarfeng.notify.stack.service.SenderInfoMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="notifySenderRelationMaintainService"
                     interface="com.dwarfeng.notify.stack.service.SenderRelationMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="notifyNotifyService"
                     interface="com.dwarfeng.notify.stack.service.NotifyService"
                     retries="3" timeout="60000" check="false"/>
</beans>
